{
  "object_template": {
    "template": "object_*", 
    "settings": {
      "analysis": {
        "analyzer": {
          "email_and_url": {
            "filter": [
              "standard", 
              "lowercase"
            ], 
            "tokenizer": "uax_url_email"
          }
        }
      }
    }, 
    "mappings": {
      "object": {
        "_all": {
          "enabled": false
        }, 
        "dynamic": "strict", 
        "properties": {
          "1:collection_id": {
            "index": "not_analyzed", 
            "type": "string"
          }, 
          "2:institution_id": {
            "index": "not_analyzed", 
            "type": "string"
          }, 
          "14:model_metadata": {
            "type": "nested", 
            "properties": {
              "1:ctime": {
                "type": "date"
              }, 
              "2:mtime": {
                "type": "date"
              }
            }
          }, 
          "3:title": {
            "type": "string"
          }, 
          "18:categories": {
            "type": "string"
          }, 
          "10:date": {
            "type": "date"
          }, 
          "11:date_text": {
            "type": "string"
          }, 
          "4:description": {
            "type": "string"
          }, 
          "19:history_notes": {
            "type": "string"
          }, 
          "17:inscriptions": {
            "type": "nested", 
            "properties": {
              "1:inscriptions": {
                "type": "nested", 
                "properties": {
                  "1:texts": {
                    "type": "nested", 
                    "properties": {
                      "1:type": {
                        "index": "not_analyzed", 
                        "type": "string"
                      }, 
                      "2:text": {
                        "type": "string"
                      }, 
                      "3:lang": {
                        "type": "string"
                      }
                    }
                  }, 
                  "2:author": {
                    "type": "nested", 
                    "properties": {
                      "1:text": {
                        "type": "string"
                      }, 
                      "2:refid": {
                        "type": "string"
                      }, 
                      "3:vocab": {
                        "index": "not_analyzed", 
                        "type": "string"
                      }
                    }
                  }, 
                  "3:position": {
                    "type": "string"
                  }
                }
              }, 
              "2:display": {
                "type": "string"
              }, 
              "3:notes": {
                "type": "string"
              }
            }
          }, 
          "15:materials": {
            "type": "nested", 
            "properties": {
              "1:materials": {
                "type": "nested", 
                "properties": {
                  "1:type": {
                    "index": "not_analyzed", 
                    "type": "string"
                  }, 
                  "2:text": {
                    "type": "string"
                  }, 
                  "3:refid": {
                    "type": "string"
                  }, 
                  "4:vocab": {
                    "index": "not_analyzed", 
                    "type": "string"
                  }
                }
              }, 
              "2:display": {
                "type": "string"
              }, 
              "3:notes": {
                "type": "string"
              }
            }
          }, 
          "12:physical_description": {
            "type": "string"
          }, 
          "5:provenance": {
            "type": "string"
          }, 
          "7:summary": {
            "type": "string"
          }, 
          "16:techniques": {
            "type": "nested", 
            "properties": {
              "1:techniques": {
                "type": "nested", 
                "properties": {
                  "1:text": {
                    "type": "string"
                  }, 
                  "2:refid": {
                    "type": "string"
                  }, 
                  "3:vocab": {
                    "index": "not_analyzed", 
                    "type": "string"
                  }
                }
              }, 
              "2:display": {
                "type": "string"
              }, 
              "3:notes": {
                "type": "string"
              }
            }
          }, 
          "8:thumbnail": {
            "type": "nested", 
            "properties": {
              "url": {
                "type": "string", 
                "analyzer": "email_and_url"
              }, 
              "height_px": {
                "type": "integer"
              }, 
              "width_px": {
                "type": "integer"
              }
            }
          }, 
          "9:url": {
            "type": "string", 
            "analyzer": "email_and_url"
          }
        }
      }
    }
  }
}