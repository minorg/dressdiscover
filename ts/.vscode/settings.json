// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/src/**/*.css.map": true,
    "**/src/**/*.js": true,
    "**/src/**/*.js.map": true,
    "worksheet/wwwroot/css/worksheet.css*": true,
    "worksheet/wwwroot/js/worksheet.js*": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/src/**/*.js": true,
    "**/src/**/*.js.map": true,
    "worksheet/wwwroot/css/worksheet.css*": true,
    "worksheet/wwwroot/js/worksheet.js*": true
  },
  "tslint.configFile": "tslint.json"
}