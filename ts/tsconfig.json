{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": ["es6", "es7", "dom"],
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "paths": {
      "dressdiscover/*": ["src/ts/dressdiscover/*"],
      "node_modules/*": ["node_modules/*"]
    },
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es5",
  }
}
