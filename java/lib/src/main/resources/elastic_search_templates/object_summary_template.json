{
  "template": "object_summary_*", 
  "settings": {
    "analysis": {
      "analyzer": {
        "email_and_url": {
          "filter": [
            "standard", 
            "lowercase"
          ], 
          "tokenizer": "uax_url_email"
        }
      }
    }
  }, 
  "mappings": {
    "object_summary": {
      "_all": {
        "enabled": false
      }, 
      "dynamic": "strict", 
      "properties": {
        "1:collection_id": {
          "index": "not_analyzed", 
          "type": "string"
        }, 
        "2:institution_id": {
          "index": "not_analyzed", 
          "type": "string"
        }, 
        "3:model_metadata": {
          "type": "nested", 
          "properties": {
            "1:ctime": {
              "type": "date"
            }, 
            "2:mtime": {
              "type": "date"
            }
          }
        }, 
        "4:title": {
          "type": "string"
        }, 
        "6:agent_name_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "7:categories": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "17:color_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "18:cultural_context_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "12:date": {
          "type": "string"
        }, 
        "5:description": {
          "type": "string"
        }, 
        "8:gender": {
          "index": "not_analyzed", 
          "type": "string"
        }, 
        "21:image": {
          "type": "nested", 
          "properties": {
            "2:full_size": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }, 
            "3:original": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }, 
            "6:rights": {
              "type": "nested", 
              "properties": {
                "1:elements": {
                  "type": "nested", 
                  "properties": {
                    "2:text": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }, 
                    "3:type": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }, 
                    "5:license_vocab_ref": {
                      "type": "nested", 
                      "properties": {
                        "2:vocab": {
                          "index": "not_analyzed", 
                          "type": "string"
                        }, 
                        "1:refid": {
                          "index": "not_analyzed", 
                          "type": "string"
                        }, 
                        "3:uri": {
                          "type": "string"
                        }
                      }
                    }, 
                    "1:rights_holder": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }, 
                    "4:notes": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }
                  }
                }, 
                "2:display": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "3:notes": {
                  "index": "not_analyzed", 
                  "type": "string"
                }
              }
            }, 
            "4:square_thumbnail": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }, 
            "5:thumbnail": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }
          }
        }, 
        "19:location_name_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "14:material_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "16:relation_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "9:subject_term_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "15:technique_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "20:structure_texts": {
          "type": "nested", 
          "dynamic": true
        }, 
        "13:url": {
          "type": "string", 
          "analyzer": "email_and_url"
        }, 
        "11:work_type_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }
      }
    }
  }
}