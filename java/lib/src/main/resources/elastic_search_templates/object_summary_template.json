{
  "template": "object_summary_*", 
  "settings": {
    "analysis": {
      "analyzer": {
        "email_and_url": {
          "filter": [
            "standard", 
            "lowercase"
          ], 
          "tokenizer": "uax_url_email"
        }
      }
    }
  }, 
  "mappings": {
    "object_summary": {
      "_all": {
        "enabled": false
      }, 
      "dynamic": "strict", 
      "properties": {
        "collection_id": {
          "index": "not_analyzed", 
          "type": "string"
        }, 
        "institution_id": {
          "index": "not_analyzed", 
          "type": "string"
        }, 
        "title": {
          "type": "string"
        }, 
        "agent_name_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "categories": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "color_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "cultural_context_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "date": {
          "type": "string"
        }, 
        "description": {
          "type": "string"
        }, 
        "gender": {
          "index": "not_analyzed", 
          "type": "string"
        }, 
        "hidden": {
          "type": "boolean"
        }, 
        "image": {
          "type": "nested", 
          "properties": {
            "2:full_size": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }, 
            "3:original": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }, 
            "6:rights": {
              "type": "nested", 
              "properties": {
                "1:elements": {
                  "type": "nested", 
                  "properties": {
                    "2:text": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }, 
                    "3:type": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }, 
                    "5:license_vocab_ref": {
                      "type": "nested", 
                      "properties": {
                        "2:vocab": {
                          "index": "not_analyzed", 
                          "type": "string"
                        }, 
                        "1:refid": {
                          "index": "not_analyzed", 
                          "type": "string"
                        }, 
                        "3:uri": {
                          "type": "string"
                        }
                      }
                    }, 
                    "4:notes": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }, 
                    "1:rights_holder": {
                      "index": "not_analyzed", 
                      "type": "string"
                    }
                  }
                }, 
                "2:display": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "3:notes": {
                  "index": "not_analyzed", 
                  "type": "string"
                }
              }
            }, 
            "4:square_thumbnail": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }, 
            "5:thumbnail": {
              "type": "nested", 
              "properties": {
                "1:url": {
                  "index": "not_analyzed", 
                  "type": "string"
                }, 
                "2:height_px": {
                  "type": "integer"
                }, 
                "3:width_px": {
                  "type": "integer"
                }
              }
            }
          }
        }, 
        "location_name_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "material_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "relation_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "structure_texts": {
          "type": "nested", 
          "dynamic": true
        }, 
        "subject_term_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "technique_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }, 
        "url": {
          "type": "string", 
          "analyzer": "email_and_url"
        }, 
        "work_type_texts": {
          "fields": {
            "text": {
              "type": "string"
            }, 
            "not_analyzed": {
              "index": "not_analyzed", 
              "type": "string"
            }
          }, 
          "type": "multi_field"
        }
      }
    }
  }
}