<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="DressDiscover.Gui.Portable.Views.Worksheet.WorksheetAccessionNumberPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <StackLayout BindingContext="{StaticResource viewModel}">
        <Label Text="Enter a new accession number:" />
        <Entry Placeholder="Accession number" Text="{Binding AccessionNumber}" />
        <Button Command="{Binding SubmitCommand}" Text="OK" />
    </StackLayout>
</ContentPage>
